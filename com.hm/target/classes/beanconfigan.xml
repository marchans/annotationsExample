<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xmlns:context="http://www.springframework.org/schema/context"
       xsi:schemaLocation="http://www.springframework.org/schema/beans 
           http://www.springframework.org/schema/beans/spring-beans-3.0.xsd
           http://www.springframework.org/schema/context
           http://www.springframework.org/schema/context/spring-context-3.0.xsd">
               
     <context:annotation-config/>

<bean id = "holiday" class="annotations.NewYear"></bean>
<bean id = "springholiday" class="annotations.Easter"></bean>
<bean id = "game" class="annotations.Monopoly"></bean>
<bean id = "alias" class = "annotations.Alias"></bean>
<bean id = "superfarm" class = "annotations.SuperFarm"></bean>
<bean id = "mobilephone" class = "annotations.MobilePhone"></bean>

</beans>
