<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans-3.0.xsd">

<bean id = "holiday" class="autowiring.NewYear" autowire = "byName"></bean>
<bean id = "springholiday" class="autowiring.Easter" autowire = "byType"></bean>
<bean id = "birthday" class="autowiring.Birthday" autowire = "constructor"></bean>
<bean id = "game" class="autowiring.Monopoly" autowire-candidate = "false" ></bean>
<bean id = "alias" class = "autowiring.Alias" autowire-candidate = "false" ></bean>
<bean id = "superfarm" class = "autowiring.SuperFarm"></bean>
<bean id = "mobilephone" class = "autowiring.MobilePhone"></bean>
<bean id = "pony" class = "autowiring.Pony"></bean>


</beans>
